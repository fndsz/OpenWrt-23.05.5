#include "mt7620a_zbtlink_zbt-we826.dtsi"

/ {
	compatible = "zbtlink,zbt-we826-e", "zbtlink,zbt-we826", "ralink,mt7620a-soc";
	model = "MMDVM_Router";

	/delete-node/ leds;

	leds {
		compatible = "gpio-leds";

		led_power: gsm {
			label = "blue:gsm";
			gpios = <&gpio0 9 GPIO_ACTIVE_LOW>;
		};

		signal {
			label = "green:signal";
			gpios = <&gpio0 11 GPIO_ACTIVE_LOW>;
		};

		sim {
			label = "red:sim";
			gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
		};

		air {
			label = "red:wifi";
			gpios = <&gpio3 0 GPIO_ACTIVE_LOW>;
		};
	};
};

&flash0 {
	broken-flash-reset;
};

&firmware {
	reg = <0x50000 0x1fb0000>;
};

&state_default {
	gpio {
		groups = "gpio", "i2c", "i2s", "refclk", "wdt", "wled_an";
		function = "gpio";
	};
};

&sdhci {
	status = "okay";
	mediatek,cd-low;
	
};

&i2c {
	status = "okay";
};

&uart1 {
	status = "okay";
};

&ehci {
	status = "okay";
	
};

&ohci {
	status = "okay";
	
};

&wmac {
	status = "okay";
};
